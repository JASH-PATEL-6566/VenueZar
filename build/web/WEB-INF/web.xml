        <servlet-name>GetUserName</servlet-name>
        <servlet-class>utils.GetUserName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchOwnerSideContent</servlet-name>
        <servlet-class>venue.FetchOwnerSideContent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchCustomerSideContent</servlet-name>
        <servlet-class>venue.FetchCustomerSideContent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingVenueServlet</servlet-name>
        <servlet-class>booking.BookingVenueServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetNumberOfNotification</servlet-name>
        <servlet-class>utils.GetNumberOfNotification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingsServlet</servlet-name>
        <servlet-class>booking.BookingsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteVenueServlet</servlet-name>
        <servlet-class>venue.DeleteVenueServlet</servlet-class>
