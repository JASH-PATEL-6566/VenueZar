    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetNumberOfNotification</servlet-name>
        <url-pattern>/GetNumberOfNotification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingsServlet</servlet-name>
        <url-pattern>/BookingsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteVenueServlet</servlet-name>
        <url-pattern>/DeleteVenueServlet</url-pattern>
    </servlet-mapping>
