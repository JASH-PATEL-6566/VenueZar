<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <welcome-file-list>
        <welcome-file>home.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>RegisterOwnerServlet</servlet-name>
        <servlet-class>login_registration.RegisterOwnerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginOwnerServlet</servlet-name>
        <servlet-class>login_registration.LoginOwnerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterCustomerServlet</servlet-name>
        <servlet-class>login_registration.RegisterCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginCustomerServlet</servlet-name>
        <servlet-class>login_registration.LoginCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RedirectBasedOnCookie</servlet-name>
        <servlet-class>cookie.RedirectBasedOnCookie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutCustomerServlet</servlet-name>
        <servlet-class>login_registration.LogoutCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutOwnerServlet</servlet-name>
        <servlet-class>login_registration.LogoutOwnerServlet</servlet-class>
    </servlet>
